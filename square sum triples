#include <stdio.h>

int main()
{
    int n;
    int a, b, c;
    int cnt = 0;

    printf("Enter value of n: ");
    scanf("%d", &n);

    for (a = 1; a <= n; a++)
    {
        for (b = 1; b <= n; b++)
        {
            for (c = 1; c <= n; c++)
            {
                if (a*a + b*b == c*c)
                {
                    cnt++;
                }
            }
        }
    }

    printf("Number of square sum triples = %d\n", cnt);

    return 0;
}
