#include <stdio.h>
int main() {
  int x;
  printf("Enter any Alphabet:\n");
  scanf("%d" ,&x);
  if(x=='A'||x=='E'||x=='I'||x=='O'||x=='U'||x=='a'||x=='e'||x=='i'||x=='o'||x=='u') {
    printf("Provided alphabet is a vowel\n"); }
    else {
      printf("Provided alphabet is a consonant\n"); }
      return 0;
}

#Expected Output:

Enter any alphabet:
r
Provided alphabet is a consonant.
